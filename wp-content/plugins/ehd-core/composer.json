{
    "name": "plugins/ehd-core",
    "description": "eHD core plugin",
    "authors": [
        {
            "name": "WEBHD Team"
        }
    ],
    "autoload": {
        "files": [
            "inc/Plugin.php"
        ],
        "psr-4": {
            "EHD\\Plugins\\Core\\": "inc/Core",
            "EHD\\Plugins\\Elementor\\": "inc/Elementor",
            "EHD\\Plugins\\Themes\\": "inc/Themes",
            "EHD\\Plugins\\Widgets\\": "inc/Widgets",
            "EHD\\Plugins\\Woocommerce\\": "inc/Woocommerce"
        },
        "exclude-from-classmap": [
            "**/Widgets/_/**"
        ]
    },
    "require": {
        "symfony/var-dumper": "^v5.4.9",
        "roots/wp-password-bcrypt": "^1.1"
    }
}