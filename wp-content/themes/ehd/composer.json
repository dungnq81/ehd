{
	"name": "wordpress/ehd",
	"description": "EHD Wordpress Theme",
	"authors": [
		{
			"name": "WEBHD Team"
		}
	],
	"autoload": {
		"files": [
			"inc/helpers.php",
			"inc/image-sizes.php",
			"inc/plugins.php",
			"inc/template-hooks.php"
		],
		"psr-4": {
			"EHD\\Sites\\Plugins\\": "inc/Plugins",
			"EHD\\Sites\\Themes\\": "inc/Themes"
		},
		"exclude-from-classmap": [
			"**/Plugins/ACF/_/**"
		]
	},
	"require": {
		"symfony/var-dumper": "^v5.4.9",
		"vectorface/whip": "^v0.4.0"
	}
}
